<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="@color/colorPrimaryBackgroundApps"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.office.template.activity.MainActivity"
    tools:showIn="@layout/app_bar_main">
    <FrameLayout
        android:id="@+id/page_fragment"
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_weight="1"
        tools:ignore="Suspicious0dp" />
    <LinearLayout
        android:orientation="horizontal"
        android:background="@color/colorPrimaryTheme"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_54sdp"
        tools:ignore="DisableBaselineAlignment">
        <LinearLayout
            android:id="@+id/linear_home"
            android:orientation="vertical"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:gravity="center"
            android:layout_gravity="center"
            android:layout_height="match_parent"
            tools:ignore="UseCompoundDrawables,UselessParent">
            <ImageView
                android:id="@+id/iv_home"
                android:src="@drawable/ic_home_enable"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:ignore="ContentDescription" />
            <TextView
                android:id="@+id/tv_home"
                android:textSize="@dimen/text_12"
                android:textColor="@color/colorPrimaryTextActive"
                android:gravity="center"
                android:fontFamily="@font/source_sans_pro_regular"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/linear_home_second"
            android:orientation="vertical"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:gravity="center"
            android:layout_gravity="center"
            android:layout_height="match_parent"
            tools:ignore="UseCompoundDrawables,UselessParent">
            <ImageView
                android:id="@+id/iv_home_second"
                android:src="@drawable/ic_home_disable"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:ignore="ContentDescription" />
            <TextView
                android:id="@+id/tv_home_second"
                android:textSize="@dimen/text_12"
                android:textColor="@color/colorPrimaryTextInactive"
                android:fontFamily="@font/source_sans_pro_regular"
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/linear_home_three"
            android:orientation="vertical"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:gravity="center"
            android:layout_gravity="center"
            android:layout_height="match_parent"
            tools:ignore="UseCompoundDrawables,UselessParent">
            <ImageView
                android:id="@+id/iv_home_three"
                android:src="@drawable/ic_home_disable"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:ignore="ContentDescription" />
            <TextView
                android:id="@+id/tv_home_three"
                android:textSize="@dimen/text_12"
                android:fontFamily="@font/source_sans_pro_regular"
                android:textColor="@color/colorPrimaryTextInactive"
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/linear_home_four"
            android:orientation="vertical"
            android:layout_weight="1"
            android:layout_width="0dp"
            android:gravity="center"
            android:layout_gravity="center"
            android:layout_height="match_parent"
            tools:ignore="UseCompoundDrawables,UselessParent">
            <ImageView
                android:id="@+id/iv_home_four"
                android:src="@drawable/ic_home_disable"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:ignore="ContentDescription" />
            <TextView
                android:id="@+id/tv_home_four"
                android:textSize="@dimen/text_12"
                android:fontFamily="@font/source_sans_pro_regular"
                android:textColor="@color/colorPrimaryTextInactive"
                android:gravity="center"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" />
        </LinearLayout>
    </LinearLayout>
</LinearLayout>