<?xml version="1.0" encoding="utf-8"?>
<androidx.core.widget.NestedScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorPrimaryWhite"
    tools:context=".fragment.register.DataDiriFragment">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@color/colorPrimaryWhite"
        tools:context=".fragment.register.AddressFragment"
        android:orientation="vertical">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="@dimen/_5sdp"
            android:backgroundTint="@color/colorPrimary"
            tools:ignore="UseCompoundDrawables,UselessLeaf">
            <ImageView
                android:id="@+id/iv_back"
                android:layout_marginStart="@dimen/_19sdp"
                android:layout_centerVertical="true"
                android:src="@drawable/ic_back_chevron"
                android:layout_gravity="center"
                android:layout_width="@dimen/_12sdp"
                android:layout_height="@dimen/_18sdp"
                tools:ignore="ContentDescription,ObsoleteLayoutParam" />
            <TextView
                android:layout_toRightOf="@+id/tv_data_diri"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/_25sdp"
                android:fontFamily="@font/nunito_bold"
                android:text="@string/tv_alamat"
                android:textColor="@color/colorPrimaryFont"
                android:textSize="@dimen/text_28"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                tools:ignore="ObsoleteLayoutParam,RtlHardcoded" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="@dimen/_133sdp"
            tools:ignore="MissingPrefix">
            <ImageView
                android:layout_gravity="top|left"
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_28sdp"
                android:src="@drawable/ic_address"
                android:layout_marginStart="@dimen/_26sdp"
                android:layout_marginTop="@dimen/_5sdp"
                tools:ignore="ContentDescription,RtlHardcoded" />
            <EditText
                android:layout_gravity="center"
                android:paddingTop="@dimen/_15sdp"
                android:id="@+id/et_nik"
                android:paddingLeft="@dimen/_14sdp"
                android:background="@drawable/design_data_diri"
                android:textColor="#999999"
                android:textSize="@dimen/text_16"
                android:gravity="top|left"
                android:inputType="number"
                android:imeOptions="actionDone"
                android:hint="@string/et_alamat_dealer"
                android:fontFamily="@font/nunito_regular"
                android:drawablePadding="@dimen/_15sdp"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_100sdp"
                android:layout_marginLeft="@dimen/_24sdp"
                android:layout_marginRight="@dimen/_24sdp"
                tools:ignore="Autofill,LabelFor,RtlHardcoded,RtlSymmetry" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="@dimen/_18sdp"
            tools:ignore="MissingPrefix">
            <ImageView
                android:layout_gravity="center"
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_28sdp"
                android:src="@drawable/ic_address"
                android:layout_marginStart="@dimen/_26sdp"
                tools:ignore="ContentDescription" />
            <EditText
                android:layout_gravity="center"
                android:id="@+id/et_full_name"
                android:paddingLeft="@dimen/_14sdp"
                android:paddingRight="@dimen/_14sdp"
                android:background="@drawable/design_data_diri"
                android:textColor="#999999"
                android:textSize="@dimen/text_16"
                android:gravity="center|left"
                android:inputType="textPersonName"
                android:imeOptions="actionDone"
                android:hint="@string/et_provinsi"
                android:fontFamily="@font/nunito_regular"
                android:drawablePadding="@dimen/_15sdp"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_50sdp"
                android:layout_marginLeft="@dimen/_24sdp"
                android:layout_marginRight="@dimen/_24sdp"
                tools:ignore="Autofill,LabelFor,RtlHardcoded" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="@dimen/_18sdp"
            tools:ignore="MissingPrefix">
            <ImageView
                android:layout_gravity="center"
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_28sdp"
                android:src="@drawable/ic_address"
                android:layout_marginStart="@dimen/_26sdp"
                tools:ignore="ContentDescription" />
            <EditText
                android:layout_gravity="center"
                android:id="@+id/et_email"
                android:paddingLeft="@dimen/_14sdp"
                android:paddingRight="@dimen/_14sdp"
                android:background="@drawable/design_data_diri"
                android:textColor="#999999"
                android:textSize="@dimen/text_16"
                android:gravity="center|left"
                android:inputType="text"
                android:imeOptions="actionDone"
                android:hint="@string/et_kabupaten"
                android:fontFamily="@font/nunito_regular"
                android:drawablePadding="@dimen/_15sdp"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_50sdp"
                android:layout_marginLeft="@dimen/_24sdp"
                android:layout_marginRight="@dimen/_24sdp"
                tools:ignore="Autofill,LabelFor,RtlHardcoded,TextFields" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="@dimen/_18sdp"
            tools:ignore="MissingPrefix">
            <ImageView
                android:layout_gravity="center"
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_28sdp"
                android:src="@drawable/ic_address"
                android:layout_marginStart="@dimen/_26sdp"
                tools:ignore="ContentDescription" />
            <EditText
                android:layout_gravity="center"
                android:id="@+id/et_address"
                android:paddingLeft="@dimen/_14sdp"
                android:paddingRight="@dimen/_14sdp"
                android:background="@drawable/design_data_diri"
                android:textColor="#999999"
                android:textSize="@dimen/text_16"
                android:gravity="center|left"
                android:inputType="text"
                android:imeOptions="actionDone"
                android:hint="@string/et_kecamatan"
                android:fontFamily="@font/nunito_regular"
                android:drawablePadding="@dimen/_15sdp"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_50sdp"
                android:layout_marginLeft="@dimen/_24sdp"
                android:layout_marginRight="@dimen/_24sdp"
                tools:ignore="Autofill,LabelFor,RtlHardcoded,TextFields" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="@dimen/_18sdp"
            tools:ignore="MissingPrefix">
            <ImageView
                android:layout_gravity="center"
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_28sdp"
                android:src="@drawable/ic_address"
                android:layout_marginStart="@dimen/_26sdp"
                tools:ignore="ContentDescription" />
            <EditText
                android:layout_gravity="center"
                android:id="@+id/et_telpon"
                android:paddingLeft="@dimen/_14sdp"
                android:paddingRight="@dimen/_14sdp"
                android:background="@drawable/design_data_diri"
                android:textColor="#999999"
                android:textSize="@dimen/text_16"
                android:gravity="center|left"
                android:inputType="text"
                android:imeOptions="actionDone"
                android:hint="@string/et_kelurahan"
                android:fontFamily="@font/nunito_regular"
                android:drawablePadding="@dimen/_15sdp"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_50sdp"
                android:layout_marginLeft="@dimen/_24sdp"
                android:layout_marginRight="@dimen/_24sdp"
                tools:ignore="Autofill,LabelFor,RtlHardcoded,TextFields" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:layout_marginTop="@dimen/_18sdp"
            tools:ignore="MissingPrefix">
            <ImageView
                android:layout_gravity="center"
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_28sdp"
                android:src="@drawable/ic_address"
                android:layout_marginStart="@dimen/_26sdp"
                tools:ignore="ContentDescription" />
            <EditText
                android:layout_gravity="center"
                android:id="@+id/et_password"
                android:paddingLeft="@dimen/_14sdp"
                android:paddingRight="@dimen/_14sdp"
                android:background="@drawable/design_data_diri"
                android:textColor="#999999"
                android:textSize="@dimen/text_16"
                android:gravity="center|left"
                android:inputType="number"
                android:imeOptions="actionDone"
                android:hint="@string/et_kode_pos"
                android:fontFamily="@font/nunito_regular"
                android:drawablePadding="@dimen/_15sdp"
                android:layout_width="match_parent"
                android:layout_height="@dimen/_50sdp"
                android:layout_marginLeft="@dimen/_24sdp"
                android:layout_marginRight="@dimen/_24sdp"
                tools:ignore="Autofill,LabelFor,RtlHardcoded,TextFields" />
        </LinearLayout>
        <Button
            android:id="@+id/btn_next_data_diri"
            android:layout_marginTop="@dimen/_56sdp"
            android:layout_marginBottom="@dimen/_20sdp"
            android:fontFamily="@font/nunito_semi_bold"
            android:textSize="@dimen/text_18"
            android:layout_marginLeft="@dimen/_20sdp"
            android:layout_marginRight="@dimen/_20sdp"
            android:textColor="@color/colorPrimaryWhite"
            android:textAllCaps="false"
            android:text="@string/btn_selanjutnya"
            android:background="@drawable/btn_design"
            android:layout_width="match_parent"
            android:layout_height="@dimen/_40sdp" />
    </LinearLayout>
</androidx.core.widget.NestedScrollView>

